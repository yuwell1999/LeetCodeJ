package Easy;

class Solution_m39 {
    public int majorityElement(int[] nums) {
        int major = 0, cnt = 0;

        for (int s : nums) {
            if (cnt == 0) {
                major = s;
            }
            if (s == major) {
                cnt++;
            } else {
                cnt--;
            }
        }
        return major;
    }
}